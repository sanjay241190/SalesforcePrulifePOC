<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>49.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <assignments>
        <name>Create_Plan_History_For_old_Plan</name>
        <label>Create Plan History For old Plan</label>
        <locationX>499</locationX>
        <locationY>50</locationY>
        <assignmentItems>
            <assignToReference>var_PlanHistory.AssetId</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>var_newPlanId</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>var_PlanHistory.RelatedAssetId</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>var_oldPlanId</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>var_PlanHistory.RelationshipType</assignToReference>
            <operator>Assign</operator>
            <value>
                <stringValue>Replacement</stringValue>
            </value>
        </assignmentItems>
    </assignments>
    <decisions>
        <name>New_and_Old_Plan_Id_Null_Check</name>
        <label>New and Old Plan Id Null Check</label>
        <locationX>224</locationX>
        <locationY>52</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Both_New_and_Old_Plan_ID_is_not_null</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>var_newPlanId</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>var_isHierarchy</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>var_oldPlanId</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Create_Plan_History_For_old_Plan</targetReference>
            </connector>
            <label>Both New and Old Plan ID is not null</label>
        </rules>
    </decisions>
    <description>This Flow Inserts the Plan History(Asset Relationship) Records whenever a new plan is added.</description>
    <interviewLabel>Plan History - PlanRelationships {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Plan History - PlanRelationships</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <startElementReference>New_and_Old_Plan_Id_Null_Check</startElementReference>
    <status>Active</status>
    <variables>
        <name>var_ExistingPlan</name>
        <dataType>SObject</dataType>
        <isCollection>false</isCollection>
        <isInput>false</isInput>
        <isOutput>false</isOutput>
        <objectType>Asset</objectType>
    </variables>
    <variables>
        <description>if This input Variable is False then the System will Insert Asset Relationship Records</description>
        <name>var_isHierarchy</name>
        <dataType>Boolean</dataType>
        <isCollection>false</isCollection>
        <isInput>true</isInput>
        <isOutput>false</isOutput>
        <value>
            <booleanValue>false</booleanValue>
        </value>
    </variables>
    <variables>
        <name>var_newPlan</name>
        <dataType>SObject</dataType>
        <isCollection>false</isCollection>
        <isInput>false</isInput>
        <isOutput>false</isOutput>
        <objectType>Asset</objectType>
    </variables>
    <variables>
        <name>var_newPlanId</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>true</isInput>
        <isOutput>false</isOutput>
    </variables>
    <variables>
        <name>var_oldPlanId</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>true</isInput>
        <isOutput>false</isOutput>
    </variables>
    <variables>
        <name>var_PlanHistory</name>
        <dataType>SObject</dataType>
        <isCollection>false</isCollection>
        <isInput>false</isInput>
        <isOutput>true</isOutput>
        <objectType>AssetRelationship</objectType>
    </variables>
    <variables>
        <name>var_PlanHistoryToInsertList</name>
        <dataType>SObject</dataType>
        <isCollection>true</isCollection>
        <isInput>false</isInput>
        <isOutput>false</isOutput>
        <objectType>AssetRelationship</objectType>
    </variables>
    <variables>
        <name>var_PlanList</name>
        <dataType>SObject</dataType>
        <isCollection>true</isCollection>
        <isInput>false</isInput>
        <isOutput>false</isOutput>
        <objectType>Asset</objectType>
    </variables>
    <variables>
        <name>var_PlanListToBeUpdated</name>
        <dataType>SObject</dataType>
        <isCollection>true</isCollection>
        <isInput>false</isInput>
        <isOutput>false</isOutput>
        <objectType>Asset</objectType>
    </variables>
</Flow>
